km = int(input('Quantos km rodados?'  ))
dias = int(input('Quantos dias rodados?'  ))

preco = 60 * dias + 0.15 * km
print(f'Km = {km}. Dias = {dias}.')
print(f'Valor a ser pago: {preco}')
